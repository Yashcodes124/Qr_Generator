{
  "name": "qr-generator",
  "version": "1.0.0",
  "description": "A simple Web-based QR generator where users can input a URL and receive a QR code.",
  "scripts": {
    "dev": "nodemon QRcify/backend/index.js",
    "start": "node QRcify/backend/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:setup": "node backend/scripts/setupDB.js",
    "migrate": "node QRcify/backend/utils/migrateDB.js",
    "reset-db": "rm -rf QRcify/database/qr_generator.sqlite && node QRcify/backend/index.js",
    "lint": "eslint 'backend/**/*.js'",
    "format": "prettier --write 'backend/**/*.js'"
  },
  "keywords": [
    "qr",
    "express",
    "node",
    "generator"
  ],
  "author": "Yashwanth",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "qr-image": "^3.2.0",
    "qrcode": "^1.5.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
